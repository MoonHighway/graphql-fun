{
  "name": "graphql-fun",
  "version": "0.1.0",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^2.0.0",
    "apollo-server-express": "^2.0.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-playground-middleware-express": "^1.7.2",
    "html-colors": "^0.0.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build:api": "webpack -p --config api.webpack.config.js",
    "build:client": "react-scripts build",
    "prestart": "yarn build:api; yarn build:client",
    "start": "node -r dotenv/config . dotenv_config_path=.env.production.local",
    "start:dev": "concurrently \"webpack --watch --config api.webpack.config.js\" \"PORT=4000 nodemon -r dotenv/config . dotenv_config_path=.env.development.local --watch src-api\" \"react-scripts start\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.6.1",
    "graphql-request": "^1.7.0",
    "nodemon": "^1.18.3",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-graphql-loader": "^1.0.0",
    "webpack-node-externals": "^1.7.2"
  }
}
